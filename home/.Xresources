! beep  on activity
URxvt.urgentOnBell:  true
XTerm*bellIsUrgent:  true

URxvt*background:	#000000
URxvt*foreground:	#EEEEEE

!!URxvt*scrollWithBuffer: true
URxvt*saveLines:		    8192
URxvt*perl-ext-common:	default,matcher
URxvt*urlLauncher:      /usr/bin/firefox
URxvt*matcher.button:	  3
!URxvt*cursorBlink:      true
!!URxvt*termName:         rxvt
URxvt*skipScroll:       false

! Disable printing
rxvt.print-pipe: "cat > /dev/null"
urxvt.print-pipe: "cat > /dev/null"

URxvt*borderColor:		#220000
URxvt*internalBorder:		1
!!URxvt*extternalBorder:		0
URxvt*scrollBar:		false
URxvt.iconFile: /usr/share/icons/Humanity/apps/128/utilities-terminal.svg

! Set fonts
URxvt*font:			xft:Monospace:regular:antialias=true:size=9
! CTRL-1 to 9 and 0 to change font size
URxvt.keysym.C-1: command: \033]710;xft:Monospace:regular:antialias=true:size=6  \007
URxvt.keysym.C-2: command: \033]710;xft:Monospace:regular:antialias=true:size=8  \007
URxvt.keysym.C-3: command: \033]710;xft:Monospace:regular:antialias=true:size=9  \007
URxvt.keysym.C-4: command: \033]710;xft:Monospace:regular:antialias=true:size=10 \007
URxvt.keysym.C-5: command: \033]710;xft:Monospace:regular:antialias=true:size=12 \007
URxvt.keysym.C-6: command: \033]710;xft:Monospace:regular:antialias=true:size=14 \007
URxvt.keysym.C-7: command: \033]710;xft:Monospace:regular:antialias=true:size=16 \007
URxvt.keysym.C-8: command: \033]710;xft:Monospace:regular:antialias=true:size=20 \007
URxvt.keysym.C-9: command: \033]710;xft:Monospace:regular:antialias=true:size=24 \007
URxvt.keysym.C-0: command: \033]710;xft:Monospace:regular:antialias=true:size=36 \007


! 0: black
